<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
 <item>
     <rotate
         android:fromDegrees="-65"
         android:toDegrees="-65"
         android:pivotX="50%"
         android:pivotY="50%"
         >
         <shape
             android:shape="line"
             android:thickness="2dp"
             >
             <stroke
                 android:width="2dp"
                 android:color="@color/blue"
                 />
         </shape>
     </rotate>
 </item>
</selector>